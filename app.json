{
  "expo": {
    "scheme": "gmeet",
    "name": "GroupMeet",
    "slug": "Groupmeet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "config": {
        "firebase": {
          "appId": "1:918392978690:web:cae30da9d0461601cf961b",
          "apiKey": "AIzaSyCwLT5t5RyiCAl5d2GlYLuoMnGE4m2uBdY",
          "measurementId": "G-RFMT5TQXBJ"
        }
      }
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "com.groupmeet.app",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "NOTIFICATIONS",
        "MEDIA_LIBRARY"
      ],
      "versionCode": 2,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "gmit",
              "host": "*.gmit.io"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "description": "Groupmeet Development version 1.98\nfix - Post sharing\nfix - chatList \nfix - chat List Notifi"
  }
}
